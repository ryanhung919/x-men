{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000 --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm test:unit && pnpm test:component && pnpm test:integration:dev",
    "test:unit": "vitest --config vitest.config.unit.ts",
    "test:component": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' vitest --config vitest.config.component.ts",
    "test:unit:deno": "deno test __tests__/unit/supabase/functions/ --allow-env --allow-net",
    "test:integration": "vitest --config vitest.config.integration.ts",
    "test:integration:dev": "concurrently --kill-others --success first \"pnpm dev\" \"wait-on http://localhost:3000 && pnpm test:integration\"",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:status": "tsx scripts/db-management.ts status",
    "db:seed": "tsx scripts/db-management.ts seed",
    "db:reset": "tsx scripts/db-management.ts reset",
    "pretest:integration": "pnpm db:status"
  },
  "dependencies": {
